{
  "name": "mongodb-anonymizer",
  "description": "Simple mongodb anonymizer",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "eslint \"./apps/**/*.ts\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start-app": "pnpm --filter=app start",
    "start-sync": "pnpm --filter=sync start",
    "start-sync:full-reindex": "pnpm --filter=sync start:full-reindex"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.5.1",
    "turbo": "^1.9.7"
  },
  "packageManager": "pnpm@7.15.0"
}
